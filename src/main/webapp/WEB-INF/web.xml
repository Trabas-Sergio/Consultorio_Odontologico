<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SvElimOdontologos</servlet-name>
        <servlet-class>servlets.SvElimOdontologos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvEditOdontologos</servlet-name>
        <servlet-class>servlets.SvEditOdontologos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvPacientes</servlet-name>
        <servlet-class>servlets.SvPacientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvElimPacientes</servlet-name>
        <servlet-class>servlets.SvElimPacientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvEditPacientes</servlet-name>
        <servlet-class>servlets.SvEditPacientes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SvCitas</servlet-name>
        <servlet-class>servlets.SvCitas</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SvElimOdontologos</servlet-name>
        <url-pattern>/SvElimOdontologos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvEditOdontologos</servlet-name>
        <url-pattern>/SvEditOdontologos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvPacientes</servlet-name>
        <url-pattern>/SvPacientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvElimPacientes</servlet-name>
        <url-pattern>/SvElimPacientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvEditPacientes</servlet-name>
        <url-pattern>/SvEditPacientes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SvCitas</servlet-name>
        <url-pattern>/SvCitas</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
